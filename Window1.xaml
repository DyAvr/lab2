<Window x:Class="lab2.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab2"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="ListBox" Margin="10,30"/>
        <Button x:Name="Return" Content="Назад" Margin="0,6,10,0" Width="186" Height="19" VerticalAlignment="Top" HorizontalAlignment="Right" Click="Return_Click"/>
        <ComboBox x:Name ="ComboBox1" Margin="0,0,10,6" Height="19" VerticalAlignment="Bottom" SelectedIndex ="0" HorizontalAlignment="Right" Width="50" Tag="" SelectionChanged="ComboBox1_SelectionChanged" >
            <ComboBoxItem Content="15" HorizontalAlignment="Center" Width="40" Height="19"/>
            <ComboBoxItem Content="30" HorizontalAlignment="Center" Width="40" Height="19"/>
            <ComboBoxItem Content="45" HorizontalAlignment="Center" Width="40" Height="19"/>
        </ComboBox>
        <TextBlock x:Name="TextBlock1" HorizontalAlignment="Left" Text ="Страница: 1"  Margin="10,0,0,6" TextWrapping="Wrap"  Width="80" Height="19" VerticalAlignment="Bottom" TextAlignment="Center"/>

    </Grid>
</Window>
